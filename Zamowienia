CREATE SCHEMA Zamowienia 

CREATE TABLE Zamowienia.Produkty(
	IDProduktu varchar(5) UNIQUE NOT NULL PRIMARY KEY,
	NazwaProduktu varchar(40),
	CenaBrutto DOUBLE(6), 
	CenaNetto DOUBLE(6)
);

CREATE TABLE Zamowienia.Dostawcy(
	IdDostawcy varchar(5) UNIQUE NOT NULL PRIMARY KEY,
	Dostawca varchar(40),
  	MiejscowoscDostawcy VARCHAR(30)
  	KodPocztowy VARCHAR(5)
	Ulica varchar(30),
	NrBudynku varchar(5)
);

CREATE TABLE Zamowienia.zamowienia(
	IDUslugi INT PRIMARY KEY UNIQUE NOT NULL,
	IDProduktu VARCHAR(4) REFERENCES Zamowienia.Produkty(IdProduktu),
	IDDostawcy VARCHAR(4) REFERENCES Zamowienia.Dostawcy(IdDostawcy)
);

